{
  "latex_to_calculator_mappings": [
    {
      "find": "\\int_\{([^}]*)\}\\\^\{([^}]*)\}\\s*([^d]*)d[a-zA-Z]",
      "replace": "y \\3,\\1,\\2)",
      "type": "regex",
      "description": "Tích phân LaTeX \int_{a}^{b} f(x) dx"
    },
    {
      "find": "\\int_\(([^)]*)\)\\\^\(([^)]*)\)\\s*([^d]*)d[a-zA-Z]",
      "replace": "y \\3,\\1,\\2)",
      "type": "regex",
      "description": "Tích phân LaTeX \int_(a)^(b) f(x) dx"
    },
    {
      "find": "int\\(([^,]+),([^,]+),([^)]+)\)",
      "replace": "y \\1,\\2,\\3)",
      "type": "regex",
      "description": "Tích phân đơn giản: int(f(x),a,b)"
    },
    {
      "find": "\\frac\{([^{}]+)\}\\{([^{}]+)\}",
      "replace": "$1a$2",
      "type": "regex",
      "description": "Chuyển phân số \\frac{x}{y} thành dạng x a y"
    },
    {
      "find": "-",
      "replace": "p",
      "type": "literal",
      "description": "Dấu âm (trừ) thành 'p'"
    },
    {
      "find": "*",
      "replace": "O",
      "type": "literal",
      "description": "Dấu nhân (*) thành 'O'"
    },
    {
      "find": "/",
      "replace": "P",
      "type": "literal",
      "description": "Dấu chia (/) thành 'P'"
    },
    {
      "find": "\\sqrt{",
      "replace": "s",
      "type": "literal",
      "description": "Hàm sqrt{ có dấu \\ → s("
    },
    {
      "find": "sqrt{",
      "replace": "s",
      "type": "literal",
      "description": "Hàm sqrt{ không dấu \\ → s("
    },
    {
      "find": "\\sqrt(",
      "replace": "s",
      "type": "literal",
      "description": "Hàm sqrt{ có dấu \\ → s("
    },
    {
      "find": "sqrt(",
      "replace": "s",
      "type": "literal",
      "description": "Hàm sqrt{ không dấu \\ → s("
    },
    {
      "find": "sin(",
      "replace": "j(",
      "type": "literal",
      "description": "Hàm sin(x) → j(x)"
    },
    {
      "find": "cos(",
      "replace": "k(",
      "type": "literal",
      "description": "Hàm cos(x) → k(x)"
    },
    {
      "find": "tan(",
      "replace": "l(",
      "type": "literal",
      "description": "Hàm tan(x) → l(x)"
    },
    {
      "find": "ln(",
      "replace": "h(",
      "type": "literal",
      "description": "Hàm ln(x) → h(x)"
    },
    {
      "find": "}",
      "replace": ")",
      "type": "literal",
      "description": "Ngoặc nhọn đóng } thành )"
    },
    {
      "find": "{",
      "replace": "(",
      "type": "literal",
      "description": "Ngoặc nhọn mở { thành ("
    },
    {
      "find": "^",
      "replace": "^",
      "type": "literal",
      "description": "Dấu mũ ^ giữ nguyên"
    },
    {
      "find": "_",
      "replace": "_",
      "type": "literal",
      "description": "Dấu chỉ số dưới _ giữ nguyên"
    },
    {
      "find": "\\left[",
      "replace": "(",
      "type": "literal",
      "description": "Thay \\left[ → ("
    },
    {
      "find": "\\right]",
      "replace": ")",
      "type": "literal",
      "description": "Thay \\right] → )"
    },
    {
      "find": "\\left(",
      "replace": "(",
      "type": "literal",
      "description": "Thay \\left( → ("
    },
    {
      "find": "\\right)",
      "replace": ")",
      "type": "literal",
      "description": "Thay \\right) → )"
    }
  ],
  "metadata": {
    "version": "1.5",
    "description": "Fix regex tích phân, thêm rule int(f(x),a,b), đặt rule tích phân lên đầu.",
    "last_updated": "2025-11-13"
  }
}
