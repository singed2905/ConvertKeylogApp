{
  "app": {
    "name": "ConvertKeylogApp",
    "version": "0.1.0",
    "author": "singed2905",
    "description": "Ứng dụng chuyển đổi keylog với architecture hiện đại"
  },
  "ui": {
    "default_theme": "default",
    "window_size": {
      "width": 1000,
      "height": 700
    },
    "auto_save_settings": true
  },
  "processing": {
    "max_file_size_mb": 100,
    "chunk_size": 1000,
    "enable_validation": true
  },
  "logging": {
    "level": "INFO",
    "file_path": "logs/app.log",
    "max_size_mb": 10,
    "backup_count": 5
  }
}
